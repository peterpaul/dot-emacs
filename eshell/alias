alias s01down nmcli con down id s01
alias hglp hgl --update --rebase && hgp
alias hgs hg status $*
alias hgc rm .hgignore; hg clean; hg revert -a
alias hgp hg push $*
alias hgl hg pull $*
alias la ls -al $*
alias ll ls -l $*
alias mvnc mvn clean $*
alias mvnci mvn clean install $*
